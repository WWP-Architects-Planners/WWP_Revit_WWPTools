<Window x:Class="WWPTools.WpfUI.TestHarness.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="WWPTools WPF Test Harness" Height="360" Width="560">
    <StackPanel Margin="24">
        <TextBlock Text="WPF Dialog Preview" FontSize="18" FontWeight="SemiBold" Margin="0,0,0,12" />
        <TextBlock Text="Click the button to open the Select Items dialog." Margin="0,0,0,16" />
        <Button x:Name="OpenDialogButton" Content="Open Select Items Dialog" Width="220" Height="32" />
    </StackPanel>
</Window>
